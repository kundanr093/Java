import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int i = 1, num;
		
		num = scan.nextInt();
		
		while(i<=num)
		{
			int j = num;

			while(j>=i)
			{
				System.out.print(num-i+1);
				j--;
			}
			System.out.println();
			i++;
			
		}

		
	}

}
