import java.util.Scanner;
public class Solution {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int num, i=1;
		char ch = 65;
		
		num = scan.nextInt();
		
		
		while(i<=num)
		{
			int j = 1;
			while(j<=i)
			{
				
				System.out.print(ch);
				
				j++;
			}
			System.out.println();
			ch++;
			i++;
		}

		
	}

}
