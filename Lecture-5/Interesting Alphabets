import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
		int num, i=1;
		char ch = 65;
		
		num = scan.nextInt();
		
		
		while(i<=num)
		{
            char c= (char)(65+num-i);
			int j = 1;
			while(j<=i)
			{
				
				System.out.print(c++);
				j++;
				
				
			}
			System.out.println();
			ch--;
			i++;
		}	
    }
}
