import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int i = 1, num;
		//int endnum = i;
		
		num = scan.nextInt();
		
		while(i<=num)
		{
			int j = i;
			while(j>=1)
			{
				System.out.print(j);
				//endnum++;
				j--;
			}
			System.out.println();
			i++;
		}

		
	}

}
